      "offering_icon_url": "data:image/svg+xml;base64,",
      "features": [
        {
          "title": "Creates an Single Instance Oracle Database on ASM.",
          "description": "Get started with Oracle Database on ASM(Automatic Storage Management) by creating and configuring an single instance."
        },
        {
          "title": "Creates an Single Instance Oracle Database on JFS2.",
          "description": "Get started with Oracle Database on AIX JFS2 by creating and configuring an single instance."
        }
      ],
